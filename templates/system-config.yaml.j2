"certificates":
  "ca": |
    {{ certificates.ca | indent(4) }}
  "{{ domain }}":
    "crt": |
      {{ certificates.crt | indent(6) }}
    "key": | 
      {{ certificates.key | indent(6) }}
"decs3o":
  "adminId": |-
    {{ decs3o.adminId }}
  "adminSecret": |-
    {{ decs3o.adminSecret }}
  "clientId": |-
    {{ subdomain }}
  "clientSecret": |-
    {{ decs3o.clientSecret }}
  "darkSite": !!bool |-
    true
  "dbName": |-
    decs3o-idms-db
  "decs3o_app_config": |-
    mode:
      darksite: true
      ldap: false
      twofactor: false
      emailoff: true
      policy: default
    server:
      host: "0.0.0.0"
      port: 8443
      cert: "/certs/{{ domain }}.crt"
      pkey: "/certs/{{ domain }}.key"
    database:
      host: "mongo"
      port: 27017
      name: "decs3o-idms-db"
    logger:
      debug: false
  "environment": |-
    {{ subdomain }}
  "jwt_public_key": |
    {{ decs3o.jwt_public_key | indent(4) }}
  "mongo_url": |-
    mongo:27017
  "port": !!int |-
    5000
  "private": !!bool |-
    true
  "server": |-
    https://sso-{{ subdomain }}.{{ domain }}:8443
  "session_timeout": !!int |-
    28800
  "ssh_port": !!int |-
    7022
